(ns mrat.midi
  (:import (github.com$jfhamlin$muscrat$pkg$midi Notes)))

;; TODO: reimplement the whole go package here.

(dorun (map
        (fn [note]
          (eval
           `(def ~(symbol (.Name note)) ~(.Frequency note))))
        Notes))

(def midi-map
  (reduce (fn [acc [m f]] (assoc acc m f))
          {}
          (map #(vector (.MIDI %) (.Frequency %)) Notes)))
