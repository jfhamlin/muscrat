(ns examples.calculating
  (:require [mrat.core :refer :all]))

(def metro (lfpulse 8 :duty 0.2))

(def freq (* 300 (semitones (* 24 (noise 8)))))

(play (-> (sin freq :duty (sin 4.1 :mul 0.4 :add 0.5))
          (* metro)
          (combc 2 0.01 3)
          (rlpf (* freq 8) 1)
          (* 0.1)))
